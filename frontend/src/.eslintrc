{
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "rules": {
    "comma-dangle": 2,
    "no-cond-assign": 2,
    "no-constant-condition": 2,
    "semi": 2,
    "space-before-function-paren": [
      "error",
      "always"
    ],
    "block-scoped-var": 2,
    "curly": [
      2,
      "all"
    ],
    "default-case": 2,
    "no-div-regex": 2,
    "no-else-return": 2,
    "no-eq-null": 2,
    "no-floating-decimal": 2,
    "no-multi-spaces": [
      2,
      {
        "exceptions": {
          "Property": true,
          "ImportDeclaration": true,
          "VariableDeclarator": true,
          "AssignmentExpression": true
        }
      }
    ],
    "no-self-compare": 2,
    "wrap-iife": [
      2,
      "inside"
    ],
    "complexity": [
      1,
      3
    ],
    "dot-notation": 1,
    "guard-for-in": 1,
    "no-extend-native": 1,
    "no-iterator": 1,
    "no-loop-func": 1,
    "no-multi-str": 1,
    "no-process-env": 1,
    "no-proto": 1,
    "no-throw-literal": 1,
    "no-unused-expressions": 1,
    "no-plusplus": 0,
    "no-underscore-dangle": 0,
    "no-mixed-operators": 0,
    "radix": 1,
    "no-alert": 0,
    "no-extra-bind": 0,
    "yoda": 0,
    "react/jsx-filename-extension": 0,
    "react/prop-types": 0,
    "react/no-multi-comp": 0,
    "class-methods-use-this": 0,
    "jsx-a11y/no-static-element-interactions": 0,
  },
  "env": {
    "browser": true,
    "es6": true
  },
  "globals": {
    "window": true,
    "$": true,
    "PDFJS": true
  },
  "parser": "babel-eslint",
  "extends": [
    "eslint:recommended",
    "airbnb"
  ]
}
